@model Agreement.Models.AgreementRecord
@{
    ViewData["Title"] = "Agreement Form";
}

<div class="container mt-4">
    <h2>Agreement Form</h2>
    <hr />

    @if (!ViewData.ModelState.IsValid)
    {
        <div class="alert alert-danger">
            <h4>Validation Errors:</h4>
            <ul>
                @foreach (var error in ViewData.ModelState.Values.SelectMany(v => v.Errors))
                {
                    <li>@error.ErrorMessage</li>
                }
            </ul>
        </div>
    }

    <form method="POST" asp-action="Create" enctype="multipart/form-data" class="needs-validation" novalidate>
        <input type="hidden" asp-for="active" value="Y" />
        <input type="hidden" asp-for="datecaputred" value="@DateTime.Now.ToString("yyyy-MM-dd")" />
        <input type="hidden" asp-for="timecaptured" value="@DateTime.Now.ToString("HH:mm:ss")" />
        <input type="hidden" asp-for="formname" value="Admission Privileges Agreement" />
        <input type="hidden" asp-for="hosigned" value="data:image/jpeg;base64," />
        @{
            string shortGuid = Guid.NewGuid().ToString("N").Substring(0, 6); // "N" removes hyphens
            string shortDate = DateTime.Now.ToString("d-M-y"); // e.g., 2-10-25 (day-month-year)
            string formId = $"Agr-{shortDate}-{shortGuid}"; // e.g., Agr-2-10-25-3a5f7d
        }
        <input type="hidden" name="formid" value="@formId" />
        
        <div class="row">
            <!-- First Name -->
            <div class="col-md-6 form-group mb-3">
                <label asp-for="drname" class="form-label">FIRST NAME *</label>
                <input asp-for="drname" class="form-control" required />
                <span asp-validation-for="drname" class="text-danger"></span>
            </div>

            <!-- Surname -->
            <div class="col-md-6 form-group mb-3">
                <label asp-for="drsurname" class="form-label">SURNAME *</label>
                <input asp-for="drsurname" class="form-control" required />
                <span asp-validation-for="drsurname" class="text-danger"></span>
            </div>
        </div>

        <!-- ID Number -->

        <div class="form-group mb-3">
            <label asp-for="dridnr" class="form-label">ID NUMBER *</label>
            <input asp-for="dridnr" class="form-control" required />
            <span asp-validation-for="dridnr" class="text-danger"></span>
            <small class="form-text text-muted">Please enter your official identification number</small>
        </div>

        <!-- bhhf Number -->
        <div class="form-group mb-3">
            <label asp-for="bhff" class="form-label">BHFF NUMBER *</label>
            <input asp-for="bhff" class="form-control" required />
            <span asp-validation-for="bhff" class="text-danger"></span>
            <small class="form-text text-muted">Please enter your official BHHF number</small>
        </div>

        <!-- Address Field with Enhanced Validation -->
        <div class="form-group mb-3">
            <label asp-for="draddress" class="form-label">Address *</label>
            <textarea asp-for="draddress"
                      class="form-control @(ViewData.ModelState["draddress"]?.Errors.Count > 0 ? "is-invalid" : "")"
                      rows="4"
                      placeholder="Street address, City, Postal Code"
                      required></textarea>
            <span asp-validation-for="draddress" class="text-danger"></span>
            <small class="form-text text-muted">Example: 123 Main St, Anytown, 12345</small>

            @if (ViewData.ModelState["draddress"]?.Errors.Count > 0)
            {
                <div class="invalid-feedback">
                    @foreach (var error in ViewData.ModelState["draddress"]?.Errors)
                    {
                        <span>@error.ErrorMessage</span>
                    }
                </div>
            }
        </div>



